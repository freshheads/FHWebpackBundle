{
    "name": "freshheads/webpack-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle to create paths to webpack assets in your Twig templates.",
    "license": "MIT",
    "authors": [
        {
            "name": "Joris van de Sande",
            "email": "joris.van.de.sande@freshheads.com",
            "homepage": "https://freshheads.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@freshheads.com",
        "issues": "https://github.com/freshheads/FHWebpackBundle/issues"
    },
    "require": {
        "php": ">=7.4",
        "symfony/asset": "^5.4 | ^6.0",
        "symfony/config": "^5.4 | ^6.0",
        "symfony/dependency-injection": "^5.4 | ^6.0",
        "symfony/event-dispatcher": "^5.4 | ^6.0",
        "symfony/http-kernel": "^5.4 | ^6.0",
        "symfony/yaml": "^5.4 | ^6.0",
        "symfony/twig-bundle": "^5.4 | ^6.0",
        "symfony/templating": "^5.4 | ^6.0",
        "twig/twig" : "^2.0 | ^3.0",
        "freshheads/webpack-stats": "^2.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4 | ^6.0"
    },
    "scripts": {
        "test": "vendor/bin/simple-phpunit --color=always --testdox"
    },
    "autoload": {
        "psr-4": { "FH\\Bundle\\WebpackBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.x-dev"
        }
    }
}

