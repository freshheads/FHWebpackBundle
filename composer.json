{
    "name": "freshheads/webpack-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle to create paths to webpack assets in your Twig templates.",
    "license": "MIT",
    "authors": [
        {
            "name": "Joris van de Sande",
            "email": "joris.van.de.sande@freshheads.com",
            "homepage": "https://freshheads.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@freshheads.com",
        "issues": "https://github.com/freshheads/FHWebpackBundle/issues"
    },
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.3.5|^5.0"
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/config": "^2.7|^3.0",
        "symfony/dependency-injection": "^2.7|^3.0",
        "symfony/event-dispatcher": "^2.7|^3.0",
        "symfony/http-kernel": "^2.7|^3.0",
        "symfony/yaml": "^2.7|^3.0",
        "symfony/twig-bundle": "^2.7|^3.0",
        "symfony/templating": "^2.7|^3.0",
        "twig/twig" : "^1.18|^2.0",
        "freshheads/webpack-stats": "^1.0"
    },
    "autoload": {
        "psr-4": { "FH\\Bundle\\WebpackBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        }
    }
}

